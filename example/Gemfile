source 'https://rubygems.org'

# You may use http://rbenv.org/ or https://rvm.io/ to install and use this version
ruby ">= 2.6.10"

# CocoaPods version must match Podfile.lock and support Xcode 26 project ISA.
gem 'cocoapods', '= 1.16.2'

# xcodeproj must be new enough to parse PBXFileSystemSynchronizedRootGroup (Xcode 26).
gem 'xcodeproj', '>= 1.26.0'

# Keep RN template constraint to avoid known issues with 7.1.0
gem 'activesupport', '>= 6.1.7.5', '!= 7.1.0'

# Keep constraint (RN templates sometimes pin it for compatibility)
gem 'concurrent-ruby', '< 1.3.4'

# Ruby 3.4.x: some stdlib parts are now shipped as gems (required by cocoapods/xcodeproj deps like CFPropertyList)
gem 'bigdecimal'
gem 'logger'
gem 'benchmark'
gem 'mutex_m'
gem 'nkf'
gem 'base64'
